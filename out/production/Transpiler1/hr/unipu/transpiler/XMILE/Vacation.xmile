				<inflow>piling_up</inflow>
				<outflow>working_down</outflow>
				<non_negative/>
			</stock>
			<flow name="piling up">
				<eqn>(1 - vacation)*(normal_workload + added_assignments)</eqn>
				<non_negative/>
			</flow>
			<flow name="working down">
				<eqn>(1 - vacation)*productivity</eqn>
				<non_negative/>
