				<non_negative/>
			</stock>
			<flow name="update\nvacation start">
				<eqn>IF DELAY(vacation, length_of_vacation - DT) = 1
THEN ((TIME - length_of_vacation + DT) - Last_Vacation_Start + DT)/DT
ELSE 0</eqn>
				<non_negative/>
			</flow>
